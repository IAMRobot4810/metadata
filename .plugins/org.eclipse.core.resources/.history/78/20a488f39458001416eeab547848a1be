#include "WPILib.h"


class RobotDemo : public IterativeTemplate{
	
	RobotDemo(){
		
	}
	
	~RobotDemo(){
		delete flTalon;
		delete frTalon;
		delete blTalon;
		delete brTalon;
		delete myRobot;
		delete stick;
	}
	
public:
	
	Talon *flTalon;
	Talon *frTalon;
	Talon *blTalon;
	Talon *brTalon;
	RobotDrive *myRobot;
	Joystick *stick;
	void RobotDemo::RobotInit();
	
private:
protected:
};

START_ROBOT_CLASS(RobotDemo);
